          <style>
            /* 
               THEME: Teraoka V2 (Japanese Industrial)
               COLORS: Green/Teal (#00A99D), Dark Grey (#333), White
            */
            :root {
                --tera-green: #00857a; /* Slightly darker for contrast */
                --tera-dark: #2c2c2c;
                --tera-light: #f4f6f6;
            }
            
            /* HERO */
            .s_tera_hero {
                position: relative;
                overflow: hidden;
            }
            .s_tera_hero .floating-box {
                position: absolute;
                bottom: 50px;
                right: 50px;
                background: rgba(255, 165, 0, 0.9); /* Orange accent from ref */
                color: white;
                max-width: 300px;
                z-index: 10;
            }
            
            /* PROBLEM GRID */
            .s_tera_problem_left {
                background-color: var(--tera-green);
                color: white;
                padding: 60px 40px;
                height: 100%;
                display: flex;
                flex-direction: column;
                justify-content: center;
            }
            .s_tera_problem_grid .problem-item {
                border: 1px solid #eee;
                padding: 30px 20px;
                text-align: center;
                transition: all 0.3s;
            }
            .s_tera_problem_grid .problem-item:hover {
                box-shadow: 0 10px 20px rgba(0,0,0,0.05);
                transform: translateY(-5px);
                border-color: var(--tera-green);
            }
            
            /* DARK CARDS */
            .s_tera_dark_card {
                background: #333;
                color: white;
                border-bottom: 3px solid #555;
                transition: 0.3s;
                height: 100%;
            }
            .s_tera_dark_card:hover {
                border-bottom-color: var(--tera-green);
            }
            
            /* UTILS */
            .text-tera-green { color: var(--tera-green) !important; }
            .bg-tera-light { background-color: var(--tera-light) !important; }
          </style>
        </div>
